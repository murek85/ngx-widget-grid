    "@webcomponents/webcomponentsjs": "2.0.0",
    "@cds/core": "6.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.7",
    "@angular/cli": "17.3.7",
    "@angular/compiler-cli": "17.3.7",
    "@angular/language-service": "17.3.7",
    "@types/jasmine": "5.1.0",
    "@types/jasminewd2": "2.0.13",
    "@types/node": "20.12.11",
    "codelyzer": "6.0.0",
